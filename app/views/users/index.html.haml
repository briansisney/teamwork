.row
  .col-md-6
    %h1#title.pane
      %i.fa.fa-user
      Users
.row
  .userlist
    - @users.each do |user|
      =render 'users/user', user: user
  .index-pane.col-md-2.pane{"data-toggle" => "modal", "data-target" => "#newUserModal"}
    %i.fa.fa-user.form-icon 
    %h4 New User
=render 'users/modal'

