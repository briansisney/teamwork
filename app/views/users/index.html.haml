.row
  .col-md-12
    #title.pane
      %i.fa.fa-user
      %h1 Users
.row
  .userlist
    - @users.each do |user|
      =render 'users/user', user: user
  .index-pane.col-xs-12.col-lg-2.col-md-3.col-sm-4.pane{"data-toggle" => "modal", "data-target" => "#newUserModal"}
    %i.fa.fa-user
    %h4 New User
=render 'users/modal'

